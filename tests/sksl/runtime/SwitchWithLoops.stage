uniform half4 colorGreen;
uniform half4 colorRed;
bool switch_with_break_in_loop_0(int x);
bool switch_with_continue_in_loop_0(int x);
bool loop_with_break_in_switch_0(int x);
bool switch_with_break_in_loop_0(int x)
{
	int val = 0;
	switch (x) 
	{
		case 1:for (int i = 0;i < 10; ++i) 
		{
			++val;
			break;
		}
		default:++val;
	}
	return val == 2;
}
bool switch_with_continue_in_loop_0(int x)
{
	int val = 0;
	switch (x) 
	{
		case 1:for (int i = 0;i < 10; ++i) 
		{
			++val;
			continue;
		}
		default:++val;
	}
	return val == 11;
}
bool loop_with_break_in_switch_0(int x)
{
	int val = 0;
	for (int i = 0;i < 10; ++i) 
	{
		switch (x) 
		{
			case 1:++val;
			break;
			default:return false;
		}
		++val;
	}
	return val == 20;
}
half4 main(float2 coords)
{
	int x = int(colorGreen.y);
	return half4((switch_with_break_in_loop_0(x) && switch_with_continue_in_loop_0(x)) && loop_with_break_in_switch_0(x) ? colorGreen : colorRed);
}
